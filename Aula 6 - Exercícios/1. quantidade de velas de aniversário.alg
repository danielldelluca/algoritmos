algoritmo "qtd velas no bolo"
// Função :
// Autor :
// Data : 22/04/2022
// Seção de Declarações 
var

   ano1: inteiro
   ano2: inteiro
   velas: inteiro
inicio

      Escreva("Em que ano estamos? ")
      Leia(ano1)
      Escreva("Em que ano eu nasci? ")
      Leia(ano2)
      velas<-ano1-ano2
      Escreva("Vou precisar de", velas, " velas para meu bolo de aniversário.")
fimalgoritmo
