algoritmo "taxa de importação"
// Função :
// Autor :
// Data : 28/04/2022
// Seção de Declarações 
var
   produto:real
   imposto:real

inicio

      Escreva("Insira o valor do produto em dólares US$: ")
      leia (produto)
      imposto<-(produto*60)/100
      Escreva("O valor do imposto em dólares será de US$ ", imposto:5:2,".")
fimalgoritmo
