algoritmo "contagem inteligente"

var
      f, i, cont: inteiro
      
inicio

Escreva("CONTAGEM INTELIGENTE")
Escreval("------------------")
Escreva("Digite um número inicial: ")
leia(i)
Escreva("Digite um número final: ")
leia(f)
EscrevaL("CONTANDO")
cont<-i
 Enquanto (cont>=f) faca
        Escreva("...", cont)
        cont<-cont-1
 FimEnquanto
fimalgoritmo
