algoritmo "contagem inteligente"

var
      n1, n2, n: inteiro
      
inicio

Escreva("CONTAGEM INTELIGENTE")
Escreval("------------------")
n<-0
Escreva("Digite um número inicial: ")
leia(n1)
Escreva("Digite um número final: ")
leia(n2)
EscrevaL("CONTANDO")
 Enquanto (n<n2) faca
        Escreva("...", n)
        n<-n+1
 FimEnquanto
fimalgoritmo
