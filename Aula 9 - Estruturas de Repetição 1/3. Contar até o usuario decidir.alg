algoritmo "contagem Ate o usario decidir"

var

   valor, contador:inteiro

inicio
   contador <- 0
   escreva("Quer contar até quanto? ")
   leia(valor)
   Enquanto (contador <= valor) faca
     EscrevaL(contador)
     contador <- contador + 1
   FimEnquanto
   Escreval("Terminei de contar")
fimalgoritmo


